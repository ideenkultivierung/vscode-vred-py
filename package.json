{
  "name": "vscode-vred-py",
  "displayName": "VRED-Py",
  "description": "Visual Studio Code Tools for Autodesk VRED",
  "version": "1.1.0",
  "publisher": "chr33z",
  "author": {
    "name": "Christopher Gebhardt",
    "email": "chr33z@gmail.com"
  },
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/chr33z/vscode-vred-py"
  },
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.44.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  }
}
